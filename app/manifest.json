{
  "name": "fayde extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Debugger for Fayde",
  "devtools_page": "devtoolsBackground.html",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/chromereload.js",
      "background/background.js"
    ]
  },
  "omnibox": {
    "keyword": "fayde extension"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
